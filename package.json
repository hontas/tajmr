{
  "name": "tajmr",
  "version": "0.4.2",
  "description": "Record some time sometime",
  "main": "main.js",
  "engines": {
    "node": "5.x"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "postinstall": "npm run build",
    "start": "node --harmony_modules --harmony_destructuring --harmony_rest_parameters --harmony_default_parameters server.js",
    "copy": "cp client/favicon.ico public && cp -a client/assets public/assets",
    "prebuild": "rm -rf public && mkdir public && npm run copy",
    "build": "NODE_ENV=production webpack --optimize-minimize",
    "postversion": "git push --follow-tags",
    "watch": "nodemon --exec 'npm start' --ignore client/ --ignore public/"
  },
  "author": "Pontus Lundin <lundin.pontus@gmail.com> (https://github.com/hontas)",
  "repository": {
    "type": "git",
    "url": "https://github.com/hontas/tajmr.git"
  },
  "license": "ISC",
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.2",
    "nodemon": "^1.8.1"
  },
  "dependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.14.1",
    "compression": "^1.6.0",
    "css-loader": "^0.23.1",
    "cuid": "^1.3.8",
    "errorhandler": "^1.4.3",
    "exp-config": "^1.3.0",
    "exports-loader": "^0.6.2",
    "express": "^4.13.3",
    "imports-loader": "^0.6.5",
    "jade": "^1.11.0",
    "json-loader": "^0.5.4",
    "morgan": "^1.6.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.0",
    "react-spinkit": "^1.1.4",
    "react-timer-mixin": "^0.13.3",
    "redux": "^3.2.1",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "socket.io": "^1.4.4",
    "style-loader": "^0.13.0",
    "stylus": "^0.52.4",
    "stylus-loader": "^1.5.0",
    "webpack": "^1.12.11",
    "webpack-dev-middleware": "^1.5.0",
    "webpack-hot-middleware": "^2.6.0",
    "whatwg-fetch": "^0.11.0"
  }
}
