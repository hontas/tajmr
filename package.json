{
  "name": "tajmr",
  "version": "1.0.0",
  "description": "Record time sometime",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "test": "jest --notify",
    "tdd": "npm run test -- --watch",
    "e2e": "npx cypress open",
    "prebuild": "rm -rf public && mkdir public",
    "build": "NODE_ENV=production webpack",
    "deploy": "npm run test && npm run build && GIT_DEPLOY_DIR=public ./scripts/deploy.sh",
    "postversion": "git push --follow-tags",
    "dev": "npm run prebuild && webpack-dev-server --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test && npx cypress"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Pontus Lundin <lundin.pontus@gmail.com> (https://github.com/hontas)",
  "repository": {
    "type": "git",
    "url": "https://github.com/hontas/tajmr.git"
  },
  "license": "ISC",
  "browserslist": [
    "last 2 Chrome versions",
    "safari 13"
  ],
  "dependencies": {
    "better-react-spinkit": "^2.0.4",
    "classnames": "^2.2.6",
    "core-js": "^3.2.1",
    "firebase": "^7.1.0",
    "promise.prototype.finally": "^3.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-axe": "^3.3.0",
    "react-day-picker": "^7.3.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "cypress": "^3.4.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "favicons-webpack-plugin": "1.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "lolex": "^4.2.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.3",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "style-loader": "^1.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "terser-webpack-plugin": "^2.1.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-pwa-manifest": "^4.0.0"
  }
}
