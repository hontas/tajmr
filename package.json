{
  "name": "tajmr",
  "version": "0.5.3",
  "description": "Record some time sometime",
  "main": "main.js",
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "postinstall": "npm run build",
    "test": "mocha --recursive --compilers js:babel-register --reporter nyan",
    "tdd": "npm test -- --watch --watch-extensions jsx --no-exit",
    "start": "node --harmony server.js",
    "copy": "cp client/sw.js public && cp client/favicon.ico public && cp -a client/assets public/assets",
    "prebuild": "rm -rf public && mkdir public && npm run copy",
    "build": "NODE_ENV=production webpack --optimize-minimize",
    "postversion": "git push --follow-tags",
    "watch": "nodemon --exec 'npm start' --ignore client/ --ignore public/"
  },
  "author": "Pontus Lundin <lundin.pontus@gmail.com> (https://github.com/hontas)",
  "repository": {
    "type": "git",
    "url": "https://github.com/hontas/tajmr.git"
  },
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^4.6.0",
    "eslint-plugin-react": "^7.3.0",
    "nodemon": "^1.9.1"
  },
  "dependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.7.4",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-0": "^6.3.13",
    "better-react-spinkit": "^2.0.0",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.0",
    "css-loader": "^0.28.7",
    "cuid": "^1.3.8",
    "errorhandler": "^1.4.3",
    "exp-config": "^1.3.1",
    "exports-loader": "^0.6.3",
    "express": "^4.13.3",
    "firebase": "^4.3.0",
    "imports-loader": "^0.7.1",
    "jade": "^1.11.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.5.0",
    "morgan": "^1.7.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-day-picker": "^6.1.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-timer-mixin": "^0.13.3",
    "redux": "^3.2.1",
    "redux-thunk": "^2.0.1",
    "socket.io": "^1.4.4",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.2",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "whatwg-fetch": "^2.0.3"
  }
}
