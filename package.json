{
  "name": "Tajmr",
  "version": "0.6.0",
  "description": "Record time sometime",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "test": "jest --notify",
    "tdd": "npm run test -- --watch",
    "copy": "cp client/sw.js public && cp -a client/assets public/assets",
    "prebuild": "rm -rf public && mkdir public && npm run copy",
    "build": "NODE_ENV=production webpack",
    "deploy": "npm run test && npm run build && GIT_DEPLOY_DIR=public ./scripts/deploy.sh",
    "postversion": "git push --follow-tags",
    "prewatch": "npm run prebuild",
    "dev": "webpack-dev-server --watch"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "author": "Pontus Lundin <lundin.pontus@gmail.com> (https://github.com/hontas)",
  "repository": {
    "type": "git",
    "url": "https://github.com/hontas/tajmr.git"
  },
  "license": "ISC",
  "dependencies": {
    "better-react-spinkit": "^2.0.4",
    "classnames": "^2.2.5",
    "firebase": "^4.9.0",
    "promise.prototype.finally": "^3.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-day-picker": "^7.0.7",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.2.1",
    "redux-thunk": "^2.0.1",
    "regenerator-runtime": "^0.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.1.18",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "compression-webpack-plugin": "^1.1.6",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jest": "^22.1.4",
    "lolex": "^2.3.2",
    "md5": "^2.2.1",
    "pre-push": "^0.1.1",
    "react-dev-utils": "^5.0.0",
    "script-ext-html-webpack-plugin": "^1.8.8",
    "style-ext-html-webpack-plugin": "^3.4.7",
    "style-loader": "^0.20.1",
    "stylus": "^0.54.2",
    "stylus-loader": "^3.0.1",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin3": "^1.0.0-beta",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.11.1",
    "webpack-hot-middleware": "^2.21.0"
  }
}
