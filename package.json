{
  "name": "tajmr",
  "version": "1.0.1",
  "description": "Record time sometime",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/tajmr/",
    "serve": "vite preview",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "test": "jest --notify",
    "tdd": "npm run test -- --watch",
    "e2e": "npx cypress open",
    "deploy": "npm run test && npm run bump && npm run build && GIT_DEPLOY_DIR=public ./scripts/deploy.sh",
    "postdeploy": "git push --follow-tags",
    "bump": "npm version patch",
    "optimize:png": "npx @squoosh/cli --oxipng auto"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test && npx cypress run"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": "Pontus Lundin <lundin.pontus@gmail.com> (https://github.com/hontas)",
  "repository": {
    "type": "git",
    "url": "https://github.com/hontas/tajmr.git"
  },
  "license": "ISC",
  "browserslist": [
    "last 2 Chrome versions",
    "safari 14"
  ],
  "dependencies": {
    "@sentry/react": "^6.2.5",
    "ajv": "^6.12.6",
    "@sentry/react": "^6.2.2",
    "classnames": "^2.2.6",
    "core-js": "^3.9.1",
    "firebase": "^8.3.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-axe": "^3.3.0",
    "react-day-picker": "^7.3.2",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "spinkit": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@sentry/webpack-plugin": "^1.9.3",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^5.1.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "cypress": "^6.6.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "favicons": "^6.2.1",
    "favicons-webpack-plugin": "^5.0.2",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lolex": "^6.0.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "^1.3.9",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-workbox": "^6.1.1",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "vite": "^2.0.5",
    "webpack": "^5.25.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^6.1.1"
  }
}
