    "stylus-loader": "^3.0.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.7.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-pwa-manifest": "^3.6.2"
  }
