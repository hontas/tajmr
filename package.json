{
  "name": "Tajmr",
  "version": "0.7.0",
  "description": "Record time sometime",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx .",
    "test": "jest --notify",
    "tdd": "npm run test -- --watch",
    "prebuild": "rm -rf public && mkdir public",
    "build": "NODE_ENV=production webpack",
    "deploy": "npm run test && npm run build && GIT_DEPLOY_DIR=public ./scripts/deploy.sh",
    "postversion": "git push --follow-tags",
    "dev": "npm run prebuild && webpack-dev-server --watch",
    "precommit": "npm run lint && npm run test"
  },
  "author": "Pontus Lundin <lundin.pontus@gmail.com> (https://github.com/hontas)",
  "repository": {
    "type": "git",
    "url": "https://github.com/hontas/tajmr.git"
  },
  "license": "ISC",
  "dependencies": {
    "better-react-spinkit": "^2.0.4",
    "classnames": "^2.2.5",
    "firebase": "^4.13.1",
    "promise.prototype.finally": "^3.1.0",
    "prop-types": "^15.6.0",
    "react": "^16.3.2",
    "react-day-picker": "^7.1.6",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.1.18",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "favicons-webpack-plugin": "0.0.9",
    "html-webpack-plugin": "^3.2.0",
